<h1>Books</h1>

<table>
 <thead>
  <tr>
  	<th>Title</th>
  	<th>Body</th>
  	<th colspan="3"></th>
  </tr>
 </thead>
 <tbody>
  <tr>
  	<% @book.each do |book| %>
  	<td></td>
  	<td></td>
  	<td>"Show"</td>
  	<td>"Edit"</td>
  	<td>"Destroy"</td>
  </tr>
 </tbody>
</table>




<h2>New book</h2>
<%= form_for(@book) do |f| %>


    <h4>Title</h4>
    <%= f.text_field :title %>

    <h4>Body</h4>
    <%= f.text_area :body %>

    <%= f.submit 'Createbook' %>

<% end %>


